{
    "name": "Rally Chrome",
    "version": "0.1",
    "description": "A Chrome extensions to easily access your Rally projects.",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "car_16.png"
          }
    },
    "icons": {
        "16": "car_16.png"
    },
    "content_security_policy": "script-src 'self' https://use.fontawesome.com/releases/v5.3.1/js/all.js; object-src 'self'",
    "manifest_version": 2
}